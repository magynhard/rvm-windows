    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^4.1.0",
    "jasmine": "^3.6.3",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "ruby-nice": "^0.1.5",
    "uglify-js": "^3.12.1",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "javascript",
    "get type",
    "check type",
    "type",
    "rvm-windows",
    "class type",
    "variable type"
