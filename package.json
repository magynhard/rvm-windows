{
  "name": "rvm-windows",
  "version": "0.0.1",
  "description": "Unoffical RVM.io port for windows",
  "exports": {
    ".": "./src/rvm-windows/rvm-windows.js",
    "./": "./src/rvm-windows/"
  },
  "main": "src/rvm-windows/rvm-windows.js",
  "types": "src/rvm-windows/rvm-windows.d.ts",
  "scripts": {
    "test": "jasmine",
    "build": "node scripts/build.js && yarn build_types",
    "build_types": "yarn tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir src/rvm-windows",
    "doc": "node scripts/doc.js"
  },
  "files": [
    "dist/rvm-windows.js",
    "src/**/*",
    "doc/**/*"
