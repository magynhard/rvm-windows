{
  "name": "rvm-windows",
  "version": "0.3.6",
  "description": "Ruby Version Manager for Windows",
  "author": "Matth√§us J. N. Beyrle <rvm-windows.npm@mail.magynhard.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magynhard/rvm-windows/issues"
  },
  "homepage": "https://github.com/magynhard/rvm-windows#readme",
  "main": "src/cli/cli.js",
  "bin": {
    "rvm": "src/cli/cli.js"
  },
  "scripts": {
    "cli": "node src/cli/cli.js",
    "doc": "node scripts/doc.js",
    "link_dev": "sudo npm link",
    "link_dev_win": "npm link",
    "test": "jasmine",
    "unlink_dev": "sudo npm rm --global rvm-windows",
    "unlink_dev_win": "npm rm --global rvm-windows",
    "relink_win": "npm run unlink_dev_win && npm run link_dev_win"
  },
  "files": [
    "src/**/*",
    "doc/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magynhard/rvm-windows.git"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "chalk": "^4.1.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "curly-bracket-parser": "^1.3.3",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "hpagent": "^1.2.0",
    "jasmine": "^5.1.0",
    "jasmine-core": "^3.10.1",
    "lucky-case": "^1.1.8",
    "node-fetch": "^2.7.0",
    "prompt-sync": "^4.2.0",
    "ruby-nice": "^0.2.8",
    "typifier": "^0.1.2",
    "undici": "^6.10.2"
  },
  "keywords": [
    "ruby",
    "version",
    "manager",
    "rvm",
    "rvm.io",
    "rubyinstaller",
    "windows",
    "rvm-windows",
    "ruby version manager",
    "native"
  ]
}
